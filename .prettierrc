{
  "semi": true,
  "singleQuote": false,
  "tabWidth": 2,
  "trailingComma": "all",
  "importOrder": [
    "^(react/(.*)$)|^(react$)",
    "^(next/(.*)$)|^(next$)",
    "^(types/(.*)$)|^(types$)",
    "<THIRD_PARTY_MODULES>",
    "",
    "^types$",
    "^@prisma/(.*)$",
    "^@/types/(.*)$",
    "^@/config/(.*)$",
    "^@/lib/(.*)$",
    "^@/hooks/(.*)$",
    "^@/components/(.*)$",
    "@workspace/ui/components/(.*)$",
    "@workspace/ui/hooks/(.*)$",
    "@workspace/ui/lib/(.*)$",
    "@workspace/ui/styles/(.*)$",
    "@workspace/database",
    "^@/actions/(.*)$",
    "^@/app/(.*)$",
    "^[./]"
  ],
  "importOrderParserPlugins": [
    "typescript",
    "jsx",
    "decorators-legacy"
  ],
  "plugins": [
    "@ianvs/prettier-plugin-sort-imports",
    "prettier-plugin-tailwindcss"
  ]
}
